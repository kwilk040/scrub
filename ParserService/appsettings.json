{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "postgres": {
    "connectionString": "Host={HOST};Database={DB_NAME};Username={USER_NAME};Password={PASSWORD}"
  }
}
